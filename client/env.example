# React App Configuration
REACT_APP_API_URL=http://localhost:5000  # Local development
REACT_APP_NAME=ShopHub
REACT_APP_VERSION=1.0.0

# API Configuration
# Only use REACT_APP_API_URL for API calls. For production, override this in .env.production.
REACT_APP_TIMEOUT=10000

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=true

# External Services (for future use)
# REACT_APP_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
# REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
# REACT_APP_CLOUDINARY_CLOUD_NAME=your_cloud_name

# Internationalization
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_LANGUAGES=en,es,hi,ta 